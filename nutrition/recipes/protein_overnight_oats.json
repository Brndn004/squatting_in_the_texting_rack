  "instructions": [
    {
      "step_id": 1,
      "text": "Combine dry ingredients: In a glass mason jar, mix together the rolled oats, chia seeds, and whey protein powder."
    },
    {
      "step_id": 2,
      "text": "Add wet ingredients: Pour in the milk and maple syrup. Stir well to ensure all ingredients are thoroughly combined."
    },
    {
      "step_id": 3,
      "text": "Refrigerate: Seal the jar and place it in the refrigerator overnight, or for at least 6 hours, to allow the oats to absorb the liquid and soften."
    },
    {
      "step_id": 4,
      "text": "Serve: Before serving, top the overnight oats with Greek yogurt and fresh blueberries."
    }
  ],
