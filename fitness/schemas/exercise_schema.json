{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["exercise_name", "1rm", "date"],
  "properties": {
    "exercise_name": {
      "type": "string"
    },
    "1rm": {
      "type": "object",
      "required": ["lb"],
      "properties": {
        "lb": {
          "type": "number"
        }
      }
    },
    "date": {
      "type": "string",
      "description": "Datetime in format YYYY-MM-DD-<unix epoch seconds>"
    }
  }
}

